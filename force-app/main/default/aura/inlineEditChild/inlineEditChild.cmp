<aura:component controller = "MarkCompetencyController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction,lightning:actionOverride" access="global">
      <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>  
      <aura:attribute name="parentdata" type="String"/>
     <aura:attribute name="sNo" type="string" />
        <aura:attribute name="objInfoForPicklistValues" type="Competency_Check__c" default="{sobjectType : 'Competency_Check__c'}" description="object information to fetch picklist values"/>
     <aura:attribute name="showSaveCancelBtn" type="boolean"/>
     <aura:attribute name="singleRec" type="List" />
    <aura:attribute name="startdateEditMode" type="boolean" default="false" /> 
     <aura:attribute name="showErrorClass" type="boolean" default="false"/>
    <aura:attribute name="showErrorClass1" type="boolean" default="false"/>
    
   <aura:attribute name="washPicklistOpts" type="string[]" description="store picklist options values"/>  
     <aura:attribute name="washEditMode" type="boolean" default="false" /> 
   
   <aura:attribute name="AvoidingBurnoutvals" type="string[]" description="store picklist options values"/>  
     <aura:attribute name="AvoidingBurnoutEditMode" type="boolean" default="false" /> 
 
    <aura:attribute name="CompleteDocumentationFormvals" type="string[]" description="store picklist options values"/>  
  <aura:attribute name="CompleteDocumentationFormEditMode" type="boolean" default="false" /> 
    
     <aura:attribute name="DressingUndressingvals" type="string[]" description="store picklist options values"/>  
  <aura:attribute name="DressingUndressingEditMode" type="boolean" default="false" /> 
   
  <aura:attribute name="managehallvals" type="string[]" description="store picklist options values"/>  
  <aura:attribute name="managehallEditMode" type="boolean" default="false" /> 
    
     <aura:attribute name="homsafvals" type="string[]" description="store picklist options values"/>  
  <aura:attribute name="homsafeEditMode" type="boolean" default="false" /> 
    
    <td ondblclick="{!c.inlineEditcontract}">
            <span class="slds-grid slds-grid_align-spread">
                <!-- show input and output section based on boolean flag -->
                <aura:if isTrue="{!v.washEditMode == false}">
                    <span class="slds-truncate" title="Status">{!v.singleRec.IHSS_AD_Complete_Documentation_Form__c}</span>
                    <button onclick="{!c.inlineEditcontract}" class="slds-button slds-button_icon slds-cell-edit__button slds-m-left_x-small" tabindex="0" title="Edit Rating">
                        <lightning:icon iconName="utility:edit" size="xx-small" alternativeText="edit"/>
                    </button>
                    <!-- Inline Edit Section in else case-->
                    <aura:set attribute="else">
                        <section  tabindex="0" class="slds-popover slds-popover_edit" role="dialog" style="position: absolute; top: 0px;width:80%">
                            <div class="slds-popover__body">
                                <div class="slds-form-element slds-grid slds-wrap">
                                    <div class="slds-form-element__control slds-grow">
                                        <label class="slds-form-element__label">IHSS A&amp;D - Complete Documentation Form</label>
                                        <ui:inputSelect aura:id="acccontract"
                                                        class="slds-select inputRatingWidth"
                                                        blur="{!c.closecontrBox}"
                                                        change="{!c.onWashChange}"
                                                        value="{!v.singleRec.IHSS_AD_Complete_Documentation_Form__c}"
                                                        />
                                    </div>
                                </div>
                            </div>
                            <span id="form-end" tabindex="0"></span>
                        </section>
                    </aura:set>
                </aura:if>
            </span>
        </td>

    
